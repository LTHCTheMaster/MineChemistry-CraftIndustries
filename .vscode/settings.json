{
	"files.exclude": {
		"**/desktop.ini": true,
		"Datapack/data/load": true,
		"Datapack/data/minecraft/tags/functions/": true,
		"**/__pycache__": true,
		"**/*.bat": true,
		".github": true,
		"periodicBuildTest": true,
		"exportTest": true,
		"templatesExport": true,
		"elementsBuildTest*": true,
		".vscode": true,
		"zip/": true,
		"**/.gitkeep": true,
		"Resourcepack/assets/lthc.chemistry/**/imported**": true,
		"Datapack/data/global": true,
		"tools/backtools/core/autofilecontainer": true,
		"requirements.txt": true
	},
	"editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
	"files.associations": {
		"base.txt": "python",
		"*.struct": "json",
		"*.pos": "log",
		"*.trslt": "fsharp",
		"linker.txt": "bat"
	},
	"datapack.env.detectionDepth": 2,
	"datapack.env.cmdVersion": "1.20.3",
	"datapack.env.dataVersion": "1.20.3",
	"[json]": {
		"editor.maxTokenizationLineLength": 32768
	}
}