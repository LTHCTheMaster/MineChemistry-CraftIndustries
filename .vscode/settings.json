{
	"files.exclude": {
		"**/desktop.ini": true,
		"Datapack/data/load": true,
		"Datapack/data/minecraft/tags/functions/load.json": true,
		"temp/.gitkeep": true,
		"**/__pycache__": true,
		"**/*.bat": true,
		".github": true,
		"periodicBuildTest": true,
		"exportTest": true,
		"elementsBuildTest": true
	},
	"editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
	"files.associations": {
		"base.txt": "python",
		"*.struct": "json",
		"*.pos": "log",
		"*.trslt": "fsharp",
		"linker.txt": "bat"
	},
	"datapack.env.detectionDepth": 2,
	"datapack.env.cmdVersion": "1.20.2",
	"datapack.env.dataVersion": "1.20.2",
	"editor.maxTokenizationLineLength": 32768
}